{
  "name": "11ty-web-template",
  "version": "1.0.0",
  "description": "Use this template for quick prototypes and simple websites.  This template uses the [Eleventy (11ty) static site generator](https://www.11ty.dev/).",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:serve": "NODE_ENV=dev npm run clean:serve",
    "dev:build": "NODE_ENV=dev npm run clean:build",
    "prod:build": "NODE_ENV=prod npm run clean:build",
    "clean": "npx rimraf docs",
    "build": "npx @11ty/eleventy",
    "clean:build": "npm run clean && npm run build",
    "serve": "npx @11ty/eleventy --serve",
    "debug": "DEBUG=* npx @11ty/eleventy --serve",
    "start": "npm run clean && npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LACMTA/11ty-web-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LACMTA/11ty-web-template/issues"
  },
  "homepage": "https://github.com/LACMTA/11ty-web-template#readme",
  "dependencies": {
    "gulp": "^4.0.2"
  },
  "devDependencies": {
    "@uswds/compile": "^1.0.0-beta.3",
    "@uswds/uswds": "^3.2.0"
  }
}
